{
  "name": "assignment",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server": "node server.js",
    "server:watch": "node --watch server.js",
    "start": "vite",
    "dev": "concurrently \"pnpm run server:watch\" \"pnpm run start\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "build": "tsc -b && vite build",
    "lint:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:tsc": "tsc --pretty",
    "lint": "pnpm lint:eslint && pnpm lint:tsc",
    "tdd:design-analysis": "node tdd-automation/design/01-run-design-analysis.js",
    "tdd:design-spec": "node tdd-automation/design/02-run-design-spec.js",
    "tdd:test-design": "node tdd-automation/test-design/03-run-test-design.js",
    "tdd:test-write": "node tdd-automation/test-write/04-run-test-write.js",
    "tdd:code-write": "node tdd-automation/code-write/05-run-code-write.js",
    "tdd:code-fix": "node tdd-automation/code-fix/06-run-code-fix.js",
    "tdd:ui-implement": "node tdd-automation/ui-implement/07-run-ui-implementation.js",
    "tdd:refactor": "node tdd-automation/refactor/08-run-refactor.js",
    "tdd:all": "pnpm tdd:design-analysis && pnpm tdd:design-spec && pnpm tdd:test-design && pnpm tdd:test-write && pnpm tdd:code-write && pnpm tdd:code-fix && pnpm tdd:ui-implement && pnpm tdd:refactor"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "7.2.0",
    "@mui/material": "7.2.0",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "framer-motion": "^12.23.0",
    "msw": "^2.10.3",
    "notistack": "^3.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^2.0.3",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.14",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "vite": "^7.0.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.2.4"
